{
    "name": "txml",
    "version": "6.0.0",
    "description": "Fastest XML DOM Parser for Node.js, Bun, Deno, and browsers",
    "type": "module",
    "main": "./src/index.js",
    "module": "./src/index.js",
    "types": "./src/index.d.ts",
    "exports": {
        ".": {
            "types": "./src/index.d.ts",
            "import": "./src/index.js",
            "default": "./src/index.js"
        },
        "./txml": {
            "types": "./src/tXml.d.ts",
            "import": "./src/tXml.js",
            "default": "./src/tXml.js"
        },
        "./transform-stream": {
            "types": "./src/transformStream.d.ts",
            "import": "./src/transformStream.js",
            "default": "./src/transformStream.js"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "test": "bun test",
        "typecheck": "tsc --noEmit"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/infalmo/tXml.git"
    },
    "keywords": [
        "xml",
        "parser",
        "dom",
        "document",
        "small",
        "fast",
        "simple",
        "bun",
        "deno"
    ],
    "author": "Tobias Nickel",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/infalmo/tXml/issues"
    },
    "homepage": "https://github.com/infalmo/tXml#readme",
    "devDependencies": {
        "bun-types": "^1.1.38",
        "typescript": "^5.7.2"
    }
}